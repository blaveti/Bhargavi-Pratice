pipeline {
    agent any

    stages {
        stage('Dev') {
            steps {
                echo 'this is the dev stage'
            }
        }
         stage('Build') {
            steps {
                echo 'this is the build stage'
            }
        }
         stage('Test') {
            steps {
                echo 'this is the testing stage'
            }
        }
         stage('Deploy') {
            steps {
                echo 'Application Deployed Successfully'
            }
        }
    }
}

